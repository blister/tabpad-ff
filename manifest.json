{
	"manifest_version": 2,
	"name": "TabPad",
	"description": "A Mozilla Firefox extension to provide a 'scratchpad' for notes on the 'New Tab' page.",
	"version": "1.0.4",
	"homepage_url": "https://github.com/blister/tabpad-ff",
	"browser_action": {
		"default_icon": "/icons/tabpad-48.png",
		"default_popup": "/popup/about.html",
		"default_title": "TabPad"
	},
	"chrome_url_overrides": {
		"newtab": "/tabpad/tabpad.html"
	},
	"permissions": [
	],
	"icons": {
		"16": "/icons/tabpad-16.png",
		"24": "/icons/tabpad-24.png",
		"48": "/icons/tabpad-48.png",
		"128": "/icons/tabpad-128.png"
	}
}
